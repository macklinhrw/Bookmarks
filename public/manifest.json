{
  "manifest_version": 3,
  "name": "Sysno Bookmarks",
  "version": "0.6",
  "description": "Bookmark search engine and database",
  "background": {
    "service_worker": "src/background/index.js",
    "type": "module"
  },
  "action": {
    "default_popup": "src/popup/index.html",
    "default_icon": "icon-34.png"
  },
  "icons": {
    "128": "icon-128.png"
  },
  "web_accessible_resources": [
    {
      "resources": [
        "icon-128.png",
        "icon-34.png"
      ],
      "matches": []
    }
  ],
  "permissions": [
    "activeTab",
    "tabs",
    "storage",
    "contextMenus",
    "notifications",
    "bookmarks"
  ]
}